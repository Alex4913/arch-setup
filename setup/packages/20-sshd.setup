#!/bin/bash
# 20-sshd.setup
# aoneill - 05/03/16

# Source the driver
source "$(basename "$(pwd)").sh"

function init() {
  section "SSH Daemon"
  
  echo "systemctl enable sshd.service"
  systemctl enable sshd.service
} 

EXEC=$(test "${BASH_SOURCE[0]}" != "${0}"; echo $?)
[[ "$EXEC" == "1" ]] && init $@
