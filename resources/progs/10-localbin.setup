#!/bin/bash
# 10-localbin.setup
# aoneill - 01/15/16

SUPPORT="$CWD/support"
GLOBALS="$SUPPORT/globals"

source "$GLOBALS"

function init() {
  section Localbin

  folder=$(get_folder "$0")
  localbin=/usr/local/bin/
  ls $folder

  echo "cp -r \"$(pwd)/$folder/*\" \"$localbin\""
  cp -r "$(pwd)/$folder/*" "$localbin"
  ls "$(pwd)/$folder/*" "$localbin"
}

EXEC=$(test "${BASH_SOURCE[0]}" != "${0}"; echo $?)
[[ "$EXEC" == "1" ]] && init $@
